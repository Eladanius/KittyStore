<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Kitty Store</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Layout/layout.css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

</head>
<body>
    <header>
        <div class="row m-0 headermenu">
            <a class="col active" href="#"><i class="fa fa-home"></i></a>
            <a class="col" href="#"><i class="fa fa-bars"></i></a>
            <a class="col" href="#"><i class="fa fa-search"></i></a>
            <a class="col" href="#"><i class="fa fa-globe"></i></a>
            <a class="col" href="#"><i class="fa fa-user-circle-o"></i></a>
            <a class="col" href="#"><i class="fa fa-shopping-basket"></i></a>

        </div>
    </header>
    <div class="body">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
